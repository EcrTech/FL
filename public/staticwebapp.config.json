{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "*.js",
      "*.css",
      "*.png",
      "*.jpg",
      "*.jpeg",
      "*.gif",
      "*.svg",
      "*.ico",
      "*.woff",
      "*.woff2",
      "*.ttf",
      "*.eot",
      "*.json",
      "*.map"
    ]
  },
  "routes": [
    {
      "route": "/digilocker/*",
      "methods": ["GET", "POST"],
      "rewrite": "/index.html",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
